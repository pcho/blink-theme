{
  "name": "nextra-theme-blinkshell",
  "version": "2.0.0-beta.22",
  "description": "Modified nextra-theme-docs for Blink Shell documentation site.",
  "repository": "https://github.com/blinksh/docs",
  "author": "Shu Ding <g@shud.in>",
  "contributors": [
    "Blink Shell <hello@blink.sh>"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "style.css"
  ],
  "sideEffects": [
    "./src/polyfill.ts"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "build": "tsup && pnpm run build:tailwind",
    "build:all": "pnpm build && pnpm run build:tailwind",
    "build:tailwind": "cross-env NODE_ENV=production pnpm postcss src/styles.css -o style.css --verbose",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "cross-env TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "prepublishOnly": "pnpm build:all",
    "test": "vitest --run",
    "clean": "rimraf ./dist ./style.css",
    "format": "prettier --ignore-path ../../.gitignore --write --list-different ."
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@mdx-js/react": "^2.1.2",
    "@reach/skip-nav": "^0.17.0",
    "clsx": "^1.2.1",
    "flexsearch": "^0.7.21",
    "focus-visible": "^5.2.0",
    "github-slugger": "^1.4.0",
    "intersection-observer": "^0.12.2",
    "match-sorter": "^6.3.1",
    "next-themes": "^0.2.0-beta.2",
    "parse-git-url": "^1.0.1",
    "scroll-into-view-if-needed": "^2.2.29",
    "title": "^3.5.1"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.4",
    "@types/flexsearch": "^0.7.3",
    "typescript": "^4.7.4",
    "@types/github-slugger": "^1.3.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "cssnano-preset-advanced": "^5.3.8",
    "next": "^12.2.4",
    "nextra": "^2.0.0-beta.18",
    "postcss": "^8.4.14",
    "postcss-cli": "^8.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "tsup": "^6.2.1",
    "vitest": "^0.21.0",
    "postcss-import": "^14.1.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
